    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script>
		$(document).ready(function(){ // START | SIDEBAR FUNCTIONS 
			$('#side #me').show();
			$('#side #menu').hide();
			$('#side #blogroll').hide();
			$('#side #twitter').hide();
			$('#side #instagram').hide();
			
			$('#controls .fa').mouseenter(function(){
				$(this).css('color', '{color:Sidebar Controls Hover}');
			});
			$('#controls .fa').mouseleave(function(){
				$(this).css('color', '{color:Sidebar Controls No Hover}');
			});
			
			
			$('#controls .fa-user').click(function(){
				$('#side #me').show();
				$('#side #menu').hide();
				$('#side #blogroll').hide();
				$('#side #twitter').hide();
				$('#side #instagram').hide();
			});
			
			$('#controls .fa-bars').click(function(){
				$('#side #menu').show();
				$('#side #me').hide();
				$('#side #blogroll').hide();
				$('#side #twitter').hide();
				$('#side #instagram').hide();
			});
			
			$('#controls .fa-heart').click(function(){
				$('#side #blogroll').show();
				$('#side #me').hide();
				$('#side #menu').hide();
				$('#side #twitter').hide();
				$('#side #instagram').hide();
			});
			
			$('#controls .fa-twitter').click(function(){
				$('#side #twitter').show();
				$('#side #me').hide();
				$('#side #menu').hide();
				$('#side #blogroll').hide();
				$('#side #instagram').hide();
			});
			
			$('#controls .fa-instagram').click(function(){
				$('#side #instagram').show();
				$('#side #me').hide();
				$('#side #menu').hide();
				$('#side #blogroll').hide();
				$('#side #twitter').hide();
			});
		}); // END | SIDEBAR FUNCTIONS
		
		$(document).ready(function(){ // SHARE BUTTON FUNCTIONS
			$('#share').hide();
			$('.share').click(function(){
				$(this).parent('.postInfo').children('#share').show();	
			})
			$('.close').click(function(){
				$(this).parent('.title').parent('#share').hide();
			});
		});
		
		$(document).ready(function(){ // SCROLL TO TOP FUNCTION
			$('#toTop').hide();
			$(document).scroll(function() {
				  var y = $(this).scrollTop();
				  if (y > 800) {
				  	  $('#toTop').fadeIn();
				  } else {
					  $('#toTop').fadeOut();
				  }
			});
			$('#toTop').click(function(){
				$('html, body').animate({scrollTop:0}, 'slow');
				return false;
			}); 
		});
	</script>
</body>
</html>
